{
	"name": "df_golden_repair_status",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_warranty",
						"type": "DatasetReference"
					},
					"name": "warranty"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_golden_repair_status",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          claim_id as string,",
				"          claim_date as date,",
				"          sales_id as string,",
				"          repair_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> warranty",
				"warranty filter(!isNull(repair_status) && trim(repair_status) != ''",
				") ~> filter1",
				"filter1 derive(repair_status_std = upper(trim(repair_status))) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          repair_status = repair_status_std",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          repair_status_key as integer,",
				"          repair_status as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          repair_status",
				"     )) ~> sink1"
			]
		}
	}
}